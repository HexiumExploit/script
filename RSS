local _0 = "1.0"
local _1 = game:GetService("Players").LocalPlayer
local _2 = game:GetService("RunService")
local _3 = _0
local _4 = { Main = { Title = "Main", Icon = "" }, Misc = { Title = "Misc", Icon = "" }, Movement = { Title = "Movement", Icon = "" }, Utility = { Title = "Utility", Icon = "" }, Settings = { Title = "Settings", Icon = "settings" } }
local _5 = nil
local _6 = false
local _7 = Vector3.new(5, 5, 5)
local _8 = Vector3.new(5, 5, 5)
local _9 = 0.7
local _a = { "LeftLowerArm", "RightLowerArm" }

local function _b(c)
    _6 = c
    if _6 then
        if _5 then _5:Disconnect() end
        _5 = _2.Stepped:Connect(function()
            local _d = _1.Character
            if _d then
                for _, _e in ipairs(_a) do
                    local _f = _d:FindFirstChild(_e)
                    if _f then
                        if _e == "LeftLowerArm" then
                            _f.Size = _7
                        elseif _e == "RightLowerArm" then
                            _f.Size = _8
                        end
                        _f.Transparency = _9
                        _f.CanCollide = false
                        _f.Massless = true
                    end
                end
            end
        end)
    else
        if _5 then _5:Disconnect() end
        local _d = _1.Character
        if _d then
            for _, _e in ipairs(_a) do
                local _f = _d:FindFirstChild(_e)
                if _f then
                    _f.Size = Vector3.new(1, 1, 1)
                    _f.Transparency = 0
                    _f.CanCollide = true
                    _f.Massless = false
                end
            end
        end
    end
end

local _g = nil
local _h = false
local _i = Vector3.new(5, 5, 5)
local _j = Vector3.new(5, 5, 5)
local _k = { "LeftFoot", "RightFoot" }

local function _l(m)
    _h = m
    if _h then
        if _g then _g:Disconnect() end
        _g = _2.Stepped:Connect(function()
            local _n = _1.Character
            if _n then
                for _, _o in ipairs(_k) do
                    local _p = _n:FindFirstChild(_o)
                    if _p then
                        if _o == "LeftFoot" then
                            _p.Size = _i
                        elseif _o == "RightFoot" then
                            _p.Size = _j
                        end
                        _p.Transparency = _9
                        _p.CanCollide = false
                        _p.Massless = true
                    end
                end
            end
        end)
    else
        if _g then _g:Disconnect() end
