local function _v(w)
    local x = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local y = x:WaitForChild("Humanoid")
    y.WalkSpeed = w and 50 or 20
end

_4.Movement:AddToggle("InfiniteStamina", {
    Title = "Infinite Stamina (Don't Tap Sprint)",
    Default = false,
    Callback = _v
})

local z = false
local A = 3
local B

local function C()
    local D = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local E = D:WaitForChild("Humanoid")
    B = game:GetService("RunService").Heartbeat:Connect(function()
        if E and E.MoveDirection.Magnitude > 0 then
            D:TranslateBy(E.MoveDirection * (A * 0.1))
        end
    end)
end

local function F()
    if B then
        B:Disconnect()
        B = nil
    end
end

_4.Movement:AddToggle("SpeedBoost", {
    Title = "Speed Boost (RISKY)",
    Default = false,
    Callback = function(G)
        if G then
            C()
        else
            F()
        end
    end
})

_4.Movement:AddSlider("SpeedSlider", {
    Title = "Speed Boost Slider",
    Default = 3,
    Min = 1,
    Max = 10,
    Rounding = 1,
    Callback = function(H) A = H end
})

local I = os.clock()
local J = tick()

local function K(L)
    local M = math.floor(L / 3600)
end
